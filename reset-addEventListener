let section = document.getElementsByTagName('section')[0];

section.addEventListener('click', (e) => {
  if (e.target.tagName == 'INPUT') { // 'INPUT must be in all caps
      if (e.target.style.backgroundColor == 'rgb(255, 255, 0)') { // if yellow
          e.target.style.backgroundColor = ''; // set bg to empty string (to default white)
      } else {
          e.target.style.backgroundColor = 'rgb(255, 255, 0)'; //set it back to yellow
      }
  } 
});
